    "name": "Relaxa",
    "card": {
      "image": "https://res.cloudinary.com/twentyninek/image/upload/f_auto,q_auto,t_global/v1652794026/New_images/meditations%20png/1024x1024/relax_your_body_hyuhyf.png"
