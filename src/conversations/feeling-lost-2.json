            "component": "TextGroup",
            "content": [
              {
                "component": "Text",
                "content": "### There are more people feeling similar to you."
              },
              {
                "component": "Text",
                "content": "* Video player carousel will come here ... *"
              }
            ]
          },
          {
            "component": "ButtonGroup",
            "content": [
              {
                "component": "Button",
                "content": "I liked the video, continue",
                "conversationId": "feeling-lost-2",
